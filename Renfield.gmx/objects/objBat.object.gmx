            <string>///Check for ground collisions


vertSpeed += baseGravity;

if(vertSpeed &gt; maxVelocity)
  vertSpeed = maxVelocity;


var vCollide;
vCollide = instance_place(x, y+vertSpeed + flyDist, objParentFloor);
if(vCollide != noone) {
  if(vCollide.wallType == SOLIDFLOOR) {
    while(!place_meeting(x,y+sign(vertSpeed) + flyDist,objParentFloor))
      y += sign(vertSpeed);
    vertSpeed = 0;
    onGround = true;
  }
  if ((vCollide.wallType == JUMPTHROUGHFLOOR) &amp;&amp; sign(vertSpeed) == 1) {
    if(!place_meeting(x,y + flyDist,objParentFloor)) {
      while(!place_meeting(x,y+sign(vertSpeed) + flyDist,objParentFloor))
        y += 1;
      vertSpeed = 0;
      onGround = true;
    } 
  }
} else {
  onGround = false;
}

y += vertSpeed;
